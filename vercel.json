{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/pokemon/(.*)",
      "destination": "/pokemon/index.html"
    },
    {
      "source": "/math/(.*)",
      "destination": "/math/index.html"
    },
    {
      "source": "/opposites/(.*)",
      "destination": "/opposites/index.html"
    },
    {
      "source": "/lightbulb/(.*)",
      "destination": "/lightbulb/index.html"
    },
    {
      "source": "/maze/(.*)",
      "destination": "/maze/index.html"
    },
    {
      "source": "/green-thumb/(.*)",
      "destination": "/green-thumb/index.html"
    }
  ],
  "headers": [
    {
      "source": "/pokemon/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/math/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/opposites/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/lightbulb/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/maze/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/green-thumb/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
